# 父类型配置
PARENT_TYPE=explorer
PARENT_NODE=root

# MySQL 数据库配置
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USER=root
MYSQL_PASSWORD=your_mysql_password
MYSQL_DATABASE=feishu

# Redis 配置（用于 Session 存储）
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0
SESSION_EXPIRE_SECONDS=86400

# 后端 URL
BACKEND_BASE=http://localhost:8000

# CORS 允许的来源（生产环境必填，多个用逗号分隔）
CORS_ORIGINS=http://localhost:5173

# YunGouOS 支付配置
YUNGOUOS_MCH_ID=你的支付宝商户号
YUNGOUOS_KEY=你的商户密钥
YUNGOUOS_NOTIFY_URL=https://你的域名/api/payment/alipay/notify

# 飞书授权码配置 (PersonalBaseToken)
PERSONAL_BASE_TOKEN=
BASE_API_DOMAIN=https://base-api.feishu.cn
